{
  "rewrites": [
    {
      "source": "/api/upload",
      "destination": "/api/upload.js"
    },
    {
      "source": "/api/files",
      "destination": "/api/files.js"
    },
    {
      "source": "/api/files/:filename",
      "destination": "/api/delete.js?filename=:filename"
    },
    {
      "source": "/api/download",
      "destination": "/api/download.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}